<?xml version="1.0"?>
<zend-config xmlns:zf="http://framework.zend.com/xml/zend-config-xml/1.0/">
		<plugin>

            <!-- the plugin name [A-Za-z_] -->
            <pluginName>GroupDocsViewerDotNet</pluginName>

            <pluginNiceName>GroupDocs Viewer for .NET</pluginNiceName>

            <!-- a short description -->
			<pluginDescription>GroupDocs Viewer for .NET plugin for PimCore CMS</pluginDescription>

            <!-- meta data -->
            <pluginVersion>1.0.0</pluginVersion>
            <pluginRevision>1</pluginRevision>
            <pluginBuildTimestamp>0</pluginBuildTimestamp>

            <!-- put in the URL for the plugin configuration eg. /plugin/GroupDocsViewerDotNet/admin/index -->
            <pluginIframeSrc></pluginIframeSrc>

            <!-- className of the plugin which extends Pimcore_API_Plugin_Abstract-->
			<pluginClassName>GroupDocsViewerDotNet_Plugin</pluginClassName>

            <!-- include paths relative to plugin-directory -->
			<pluginIncludePaths>
				<path>/GroupDocsViewerDotNet/lib</path>
                <path>/GroupDocsViewerDotNet/models</path>
			</pluginIncludePaths>

			<!-- namespaces to register with autoloader-->
			<pluginNamespaces>
				<namespace>GroupDocsViewerDotNet</namespace>
			</pluginNamespaces>

			<!-- js files with path relative to plugin-directory -->
			<pluginJsPaths>
                <path>/plugins/GroupDocsViewerDotNet/static/js/admin.js</path>
			</pluginJsPaths>

			<!-- css files with path relative to plugin-directory -->
			<pluginCssPaths>
                <path>/plugins/GroupDocsViewerDotNet/static/css/admin.css</path>
			</pluginCssPaths>
		</plugin>
</zend-config>
